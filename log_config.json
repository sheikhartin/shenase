{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "console": {
      "format": "%(levelname)8s %(asctime)s - %(message)s"
    },
    "file": {
      "format": "%(levelname)8s %(levelname)s %(asctime)s - %(message)s"
    }
  },
  "handlers": {
    "console": {
      "level": "DEBUG",
      "formatter": "console",
      "class": "logging.StreamHandler",
      "stream": "ext://sys.stdout"
    },
    "file": {
      "level": "WARNING",
      "formatter": "file",
      "class": "logging.FileHandler",
      "filename": "server.log"
    }
  },
  "loggers": {
    "uvicorn": {
      "handlers": ["console", "file"],
      "level": "DEBUG",
      "propagate": false
    }
  }
}
